<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:vm="clr-namespace:FrostyEditor.ViewModels.Windows"
        xmlns:pages="clr-namespace:FrostyEditor.Views.Pages.Windows.ThemeManagerWindow"
        xmlns:fluentAvalonia="using:FluentAvalonia.UI.Controls"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="FrostyEditor.Views.Windows.ThemeManagerWindow"
        x:DataType="vm:ThemeManagerWindowViewModel"
        Title="ThemeManagerWindow">
  <Design.DataContext>
    <vm:ThemeManagerWindowViewModel />
  </Design.DataContext>
  <Panel>
    <Grid Background="{DynamicResource AcrylicFallbackColor}">
      <TabControl>
        <!-- Library -->
        <TabItem Header="Library">
          <fluentAvalonia:Frame x:Name="LibraryFrame" />
        </TabItem>
        <TabItem Header="Browse">
          <Label Content="TODO, maybe connect to github"/>
        </TabItem>
      </TabControl>
    </Grid>
  </Panel>
</Window>
